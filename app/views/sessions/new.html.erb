<%= form_for :session, url: sign_in_path do |f| %>
  <div class="field">
    <%= f.label :login%><br/>
    <%= f.number_field :login, autofocus: true %>
  </div>
  <div class="field">
    <%= f.label :password%><br/>
    <%= f.password_field :password, autocomplete: "off" %>
  </div>
  <div class="button">
    <%= f.submit "Acessar" %>
  </div>
<% end %>